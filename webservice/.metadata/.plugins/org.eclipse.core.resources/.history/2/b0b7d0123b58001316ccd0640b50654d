package resources;

import javax.ws.rs.Path;
@Path("posts/{ID}/comments")
public class CommentResource {
	
	@GET
	@Produces(MediaType.Application_JSON)
	
	@POST
	@Consumes(MediaType.APPLICATION_JSON)
	//create new comments and generate ID
	
	@PUT
	@Consumes(MediaType.APPLICATION_JSON)

	@DELETE
	@Consumes(MediaType.APPLICATION_JSON)

}
